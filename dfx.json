{
  "canisters": {
    "CandidSpaces": {
      "main": "motoko/service/CandidSpaces.mo"
    }
   },
  "defaults": {
    "build": {
      "packtool": "vessel sources",
      "output": "build"
    },
    "start": {
      "address": "127.0.0.1",
      "port": 8000,
      "serve_root": "build"
    }
  },
  "networks": {
    "alpha": {
      "providers": [
        "https://mainnet.dfinity.network"
      ],
      "type": "persistent"
    },
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "dfx": "0.7.0"
}
